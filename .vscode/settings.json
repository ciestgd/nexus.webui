{
  "prettier.singleQuote": true,
  "prettier.semi": true,
  "prettier.tabWidth": 4,
  "prettier.printWidth": 180,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/dist": true,
    "auto-imports.d.ts": true,
    "components.d.ts": true,
    "src/components/shims-vue.d.ts": true,
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "auto-imports.d.ts": true,
    "components.d.ts": true,
    "src/components/shims-vue.d.ts": true,
  },
  "path-intellisense.mappings": {
    "@/": "${workspaceFolder}/src"
  }
}